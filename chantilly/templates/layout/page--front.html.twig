{#
* Regions:
* - page.header: Items for the header region.
* - page.primary_menu: Items for the Primary Menu region.
* - page.content: The main content of the current page.
* - page.main_right: Items for the Main Right region.
* - page.left_bottom: Items for the Left Bottom region.
* - page.left_top: Items for the Left Top region.
* - page.main_left: Items for the Main Left region.
* - page.right_bottom: Items for the Right Bottom region.
* - page.right_top: Items for the Right Top region.
* - page.footer_branding: Items for the footer branding region.
* - page.footer_text: Items for the footer text region.
* - page.footer_menu: Items for the footer menu region.
* - page.footer_menu: Items for the footer menu region.
* - page.one_one: Items for the footer one-one region.
* - page.one_two: Items for the footer one-two region.
* - page.one_three: Items for the footer one-three region.
* - page.two_one: Items for the footer two-one region.
* - page.two_two: Items for the footer two-two region.
* - page.two_three: Items for the footer two-three region.
* - page.three_one: Items for the footer three-one region.
* - page.three_two: Items for the footer three-two region.
* - page.three_three: Items for the footer three-three region.
#}

<header id="header" class="container header">
	<div class="row margin-bottom-1-5">
	{% if page.header %}
		<div class="col-lg-4 col-md-4 col-sm-5 col-sx-12 margin-top-point5 margin-bottom-point5">
			{{ page.header }}
		</div>
	{% endif %}
	{% if page.primary_menu %}
		<div class="col-lg-8 col-md-8 col-sm-7 col-sx-12">
			<div class="row margin-top-1 float-right">
				{{ page.primary_menu }}
			</div>
		</div>
	{% endif %}
	</div>
</header>

<div class="container">
	<div class="row margin-top-1">
		{% if page.left_top or page.left_bottom %}
			<div id="left-sidebar" class="col-lg-4 col-md-4 col-sm-4 col-sx-12">
				{% if page.left_top %}
						{{ page.left_top }}
				{% endif %}
				{% if page.left_bottom %}
					<div class="margin-top-1dot7">
						{{ page.left_bottom }}
					</div>
				{% endif %}
			</div>
		{% endif %}
		{% if page.left_top or page.left_bottom %}
			<div id="main-right" class="col-lg-8 col-md-8 col-sm-8 col-sx-12">
		{% else %}
			<div id="main-right" class="col-lg-12 col-md-12 col-sm-12 col-sx-12">
		{% endif %}
			{{ page.main_right }}
			{{ page.content }}
		</div>
	</div>
</div>

<div id="mid-region" class="containter-fluid grid-background-mid">
	<div class="container margin-top-5">
		{% if page.right_bottom or page.right_top or page.main_left %}
		<div class="row margin-top-3">
			{% if page.main_left %}
				{% if page.right_bottom or page.right_top %}
					<div id="main-left" class="col-lg-9 col-md-9 col-sm-8 col-sx-12 main-left padding-0">
				{% else %}
					<div id="main-left" class="col-lg-12 col-md-12 col-sm-12 col-sx-12 main-left square">
				{% endif %}
						{{ page.main_left}}
					</div>
			{% endif %}
			{% if page.right_bottom or page.right_top %}
				<div class="col-lg-3 col-md-3 col-sm-4 col-sx-12">
					{% if page.right_top %}
						<div id="right-top">
							{{ page.right_top }}
						</div>
					{% endif %}
					{% if page.right_bottom %}
						<div id="right-bottom">
							{{ page.right_bottom }}
						</div>
					{% endif %}
				</div>
			{% endif %}
		</div>
		{% endif %}
	</div>
</div>

<div class="container-fluid bottom">
	{% if page.one_one or page.one_two or page.one_three %}
		<div class="row margin-top-5">
			{% if page.one_one %}
				<div class="col-lg-4 col-md-4 col-sm-4 col-sx-12 padding-0">
					{{ page.one_one }}
				</div>
			{% endif %}
			{% if page.one_two %}
				<div class="col-lg-4 col-md-4 col-sm-4 col-sx-12 padding-0">
					{{ page.one_two }}
				</div>
			{% endif %}
			{% if page.one_three %}
				<div class="col-lg-4 col-md-4 col-sm-4 col-sx-12 padding-0">
					{{ page.one_three }}
				</div>
			{% endif %}
		</div>
	{% endif %}
	{% if page.two_one or page.two_two or page.two_three %}
		<div class="row">
			{% if page.two_one %}
				<div class="col-lg-4 col-md-4 col-sm-4 col-sx-12 padding-0">
					{{ page.two_one }}
				</div>
			{% endif %}
			{% if page.two_two %}
				<div class="col-lg-4 col-md-4 col-sm-4 col-sx-12 padding-0">
					{{ page.two_two }}
				</div>
			{% endif %}
			{% if page.two_three %}
				<div class="col-lg-4 col-md-4 col-sm-4 col-sx-12 padding-0">
					{{ page.two_three }}
				</div>
			{% endif %}
		</div>
	{% endif %}
	{% if page.two_one or page.two_two or page.two_three %}
		<div class="row">
			{% if page.three_one %}
				<div class="col-lg-4 col-md-4 col-sm-4 col-sx-12 padding-0">
					{{ page.three_one }}
				</div>
			{% endif %}
			{% if page.three_two %}
				<div class="col-lg-4 col-md-4 col-sm-4 col-sx-12 padding-0">
					{{ page.three_two }}
				</div>
			{% endif %}
			{% if page.three_three %}
				<div class="col-lg-4 col-md-4 col-sm-4 col-sx-12 padding-0">
					{{ page.three_three }}
				</div>
			{% endif %}
		</div>
	{% endif %}
</div>

<footer class="footer margin-4 grid-background-footer">
	<div class="menu-symbol margin-bottom-1">
		{{ page.footer_branding }}
	</div>
	<div class="row">
		<div class="col-lg-6 col-md-6 col-sm-12 col-sx-12 margin-top-1">
			{{ page.footer_text }}
		</div>
		<div class="col-lg-6 col-md-6 col-sm-12 col-sx-12 margin-top-1">
			<div id="social-media-links" class="row display-flex">
				{{ page.footer_menu }}
			</div>
		</div>
	</div>
</footer>