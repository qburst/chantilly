{#
/**
 * @file
 * Theme override for a views mini-pager.
 *
 * Available variables:
 * - heading_id: Pagination heading ID.
 * - items: List of pager items.
 *
 * @see template_preprocess_views_mini_pager()
 */
#}
{% if items.previous or items.next %}
  <nav role="navigation" aria-labelledby="{{ heading_id }}">
    <ul style="padding-left:0em;">
      {% if items.previous %}
        <a href="{{ items.previous.href }}" title="{{ 'Go to previous page'|t }}" rel="prev"{{ items.previous.attributes|without('href', 'title', 'rel') }}>
          <span class="visually-hidden">{{ 'Previous page'|t }}</span>
          <span aria-hidden="true">
            <img src="/themes/chantilly/images/group-40.png" class="margin-top-1 arrow-circle">
          </span>
        </a>
      {% else %}
        <span aria-hidden="true">
          <img src="/themes/chantilly/images/group-40.png" class="margin-top-1 arrow-circle" style="opacity: 50%;">
        </span>
      {% endif %}
      &nbsp;&nbsp;
      {% if items.next %}
        <a href="{{ items.next.href }}" title="{{ 'Go to next page'|t }}" rel="next"{{ items.next.attributes|without('href', 'title', 'rel') }}>
          <span class="visually-hidden">{{ 'Next page'|t }}</span>
          <span aria-hidden="true">
            <img src="/themes/chantilly/images/group-39.png" class="margin-top-1 arrow-circle">
          </span>
        </a>
      {% else %}
        <span aria-hidden="true">
          <img src="/themes/chantilly/images/group-39.png" class="margin-top-1 arrow-circle" style="opacity: 50%;">
        </span>
      {% endif %}
    </ul>
  </nav>
{% endif %}
